
cd $job_results
$run_prefix

if [ -z "$facs_location" ]
then
	echo "Please set $$facs_location in your deploy/machines_user.yml file."
else
	export PYTHONPATH=$facs_location:$$PYTHONPATH
fi

/usr/bin/env > env.log

start_time="$$(date -u +%s.%N)"


python3 $facs_location/run.py --location=$location --generic_outfile --ci_multiplier=$ci_multiplier --transition_scenario=$transition_scenario --transition_mode=$transition_mode --output_dir=$output_dir

end_time="$$(date -u +%s.%N)"
elapsed="$$(bc <<<"$$end_time-$$start_time")"
echo "Total Executing Time = $$elapsed seconds"

